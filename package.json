{
  "name": "@mlaursen/prettier-config",
  "type": "module",
  "version": "0.0.2",
  "description": "The reusable prettier config I normally use for repos",
  "repository": "https://github.com/mlaursen/prettier-config.git",
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "typecheck": "tsc --noEmit",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "release": "tsx scripts/release.ts"
  },
  "files": [
    "./index.js",
    "./index.d.ts"
  ],
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
  "bugs": {
    "url": "https://github.com/mlaursen/prettier-config/issues"
  },
  "keywords": [
    "prettier",
    "config",
    "mlaursen"
  ],
  "dependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@mlaursen/release-script": "^0.0.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "prettier": ">= 3.6.2 < 4"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,md,yml,yaml,json}": [
      "prettier --write"
    ]
  },
  "volta": {
    "node": "22.18.0",
    "pnpm": "10.14.0"
  }
}
